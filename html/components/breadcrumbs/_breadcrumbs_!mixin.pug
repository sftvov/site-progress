mixin breadcrumbs(props)
  .breadcrumbs
    .container
      .breadcrumbs__body
        a.breadcrumbs__item(href="/page-main.html") Главная
        each b, index in props
          if index === props.length - 1
            span.breadcrumbs__item !{b}
          else
            a.breadcrumbs__item(href="#") !{b}